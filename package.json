{
  "name": "openchia",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "build": "ng build --configuration=production; npm run scully:all; gzipper compress --brotli --include js,css,html --level 6 ./dist/static",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "scully": "npx scully --",
    "scully:all": "for i in en cs de es fr hu pl pt sk ru zh-CN; do env LOCALE=$i scully --scanRoutes; done",
    "scully:serve": "npx scully serve --"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "12.1.2",
    "@angular/compiler": "12.1.2",
    "@angular/core": "12.1.2",
    "@angular/forms": "12.1.2",
    "@angular/localize": "12.1.2",
    "@angular/platform-browser": "12.1.2",
    "@angular/platform-browser-dynamic": "12.1.2",
    "@angular/router": "12.1.2",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@scullyio/init": "^1.1.4",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.1.1-BETA.62759",
    "@scullyio/scully-plugin-base-href-rewrite": "^1.1.1",
    "@swimlane/ngx-charts": "^18.0.1",
    "angular-csv-ext": "^1.0.5",
    "jw-bootstrap-switch-ng2": "^2.0.5",
    "moment": "^2.29.1",
    "ng-terminal": "^3.1.0",
    "ng2-nouislider": "1.8.2",
    "ngx-filesize": "^2.0.16",
    "nouislider": "14.6.3",
    "popper.js": "1.16.1",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "scully-plugin-minify-html": "^6.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.1.2",
    "@angular/cli": "^12.1.2",
    "@angular/compiler-cli": "12.1.2",
    "@angular/language-service": "12.1.2",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.19",
    "codelyzer": "6.0.1",
    "gzipper": "^5.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  }
}
